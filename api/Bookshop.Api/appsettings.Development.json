{
  "ConnectionStrings": {
    "BookshopDBConnectionString": "Server=localhost;Database=BookshopDB;Trusted_Connection=True;MultipleActiveResultSets=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "@mt = 'An unhandled exception has occurred while executing the request.'"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "https://localhost:5001" }
      }
    ],
    "Properties": {
      "ApplicationName": "Serilog.WebApplication"
    }
  },
  "JwtSettings": {
    "Key": "MkI3NEQ0RjdCNkVGRDNBMUU4ODk4NEQ2QzkzMjk4QTJDRTU3MkZEQkRCNTE1NDc2RjY5MzJCMzg1QUU5M0I2RQ==",
    "Issuer": "localhost",
    "Audience": "localhost",
    "DurationInMinutes": 60
  },
  "AllowedOrigins": [
    "http://localhost:4200"
  ]
}
